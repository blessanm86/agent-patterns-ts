# The Ollama model to use. Swap to qwen2.5:14b anytime for a smarter model.
MODEL=qwen2.5:7b

# Ollama host (default is fine if running locally)
OLLAMA_HOST=http://localhost:11434

# Embedding model for RAG (concept 10)
EMBEDDING_MODEL=nomic-embed-text

# Model tiers for cost tracking (concept 20)
FAST_MODEL=qwen2.5:1.5b
CAPABLE_MODEL=qwen2.5:14b
